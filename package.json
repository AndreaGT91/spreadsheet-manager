{
  "name": "build-a-base",
  "version": "2.0.0",
  "description": "Allows non-technical users to create databases from spreadsheets",
  "private": true,
  "homepage": "https://github.com/AndreaGT91/spreadsheet-manager/#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/AndreaGT91/spreadsheet-manager.git"
  },
  "engines": {
    "node": ">14.0.0",
    "npm": ">6.0.0"
  },
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "author": "Bentley/Jones",
  "contributors": [
    {
      "name": "Andrea Bentley",
      "url": "http://bentleytech.com"
    },
    {
      "name": "Katherine Jones",
      "url": "https://thedevkat.com/"
    }
  ],
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.7"
  },
  "dependencies": {
    "@apollo/client": "^3.1.2",
    "@restart/hooks": "^0.3.25",
    "axios": "^0.18.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "bootstrap": "^4.5.2",
    "express": "^4.16.3",
    "fs": "0.0.1-security",
    "if-env": "^1.0.4",
    "is-empty": "^1.2.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.27",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "validator": "^13.1.1"
  }
}
